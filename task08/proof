vagrant@wheezy:~$ declare FOO=/sys/kernel/debug/eudyptula/foo
vagrant@wheezy:~$ 
vagrant@wheezy:~$ # Mount debugfs
vagrant@wheezy:~$ sudo mount -t debugfs none /sys/kernel/debug
vagrant@wheezy:~$ sudo chmod 0755 /sys/kernel/debug
vagrant@wheezy:~$ sudo insmod /vagrant/task08.ko
vagrant@wheezy:~$ 
vagrant@wheezy:~$ # Read/write to id file
vagrant@wheezy:~$ cat /sys/kernel/debug/eudyptula/id; echo
5d658d788cc9
vagrant@wheezy:~$ echo $(cat /sys/kernel/debug/eudyptula/id) > /sys/kernel/debug/eudyptula/id
vagrant@wheezy:~$ echo $?
0
vagrant@wheezy:~$ 
vagrant@wheezy:~$ # Show jiffies
vagrant@wheezy:~$ cat /sys/kernel/debug/eudyptula/jiffies
4294924716
vagrant@wheezy:~$ sleep 1
vagrant@wheezy:~$ cat /sys/kernel/debug/eudyptula/jiffies
4294924968
vagrant@wheezy:~$ 
vagrant@wheezy:~$ # Test foo file
vagrant@wheezy:~$ ## Normal users can read, not write
vagrant@wheezy:~$ cat $FOO
vagrant@wheezy:~$ echo "abcd" > $FOO
-bash: /sys/kernel/debug/eudyptula/foo: Permission denied
vagrant@wheezy:~$ 
vagrant@wheezy:~$ ## Root can write
vagrant@wheezy:~$ echo "abcd" | sudo tee $FOO
abcd
vagrant@wheezy:~$ cat $FOO
abcd
vagrant@wheezy:~$ echo "xyz" | sudo tee $FOO
xyz
vagrant@wheezy:~$ cat $FOO
xyz
vagrant@wheezy:~$ 
vagrant@wheezy:~$ ## Write up to one page size
vagrant@wheezy:~$ python -c 'import sys; sys.stdout.write("A" * 4095)' | sudo tee $FOO
@wheezy:~$ cat $FOO; echo

vagrant@wheezy:~$ python -c 'import sys; sys.stdout.write("A" * 4097)' | sudo tee $FOO
: /sys/kernel/debug/eudyptula/foo: Invalid argument
Avagrant@wheezy:~$ 
vagrant@wheezy:~$ # Clean up all eudyptula debugfs files
vagrant@wheezy:~$ sudo rmmod task08.ko
vagrant@wheezy:~$ ls -l /sys/kernel/debug/eudyptula
ls: cannot access /sys/kernel/debug/eudyptula: No such file or directory
