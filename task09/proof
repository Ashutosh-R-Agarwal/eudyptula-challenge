$ declare FOO=/sys/kernel/eudyptula/foo
vagrant@wheezy:/vagrant$ 
vagrant@wheezy:/vagrant$ sudo insmod /vagrant/task09.ko
vagrant@wheezy:/vagrant$ 
vagrant@wheezy:/vagrant$ # Read/write to id file
vagrant@wheezy:/vagrant$ cat /sys/kernel/eudyptula/id; echo
5d658d788cc9

vagrant@wheezy:/vagrant$ echo $(cat /sys/kernel/eudyptula/id) | sudo tee /sys/kernel/eudyptula/id
5d658d788cc9
vagrant@wheezy:/vagrant$ echo $?
0
vagrant@wheezy:/vagrant$ 
vagrant@wheezy:/vagrant$ # Show jiffies
vagrant@wheezy:/vagrant$ cat /sys/kernel/eudyptula/jiffies
4294913929
vagrant@wheezy:/vagrant$ sleep 1
vagrant@wheezy:/vagrant$ cat /sys/kernel/eudyptula/jiffies
4294914182
vagrant@wheezy:/vagrant$ 
vagrant@wheezy:/vagrant$ # Test foo file
vagrant@wheezy:/vagrant$ ## Normal users can read, not write
vagrant@wheezy:/vagrant$ cat $FOO
vagrant@wheezy:/vagrant$ echo "abcd" > $FOO
-bash: /sys/kernel/eudyptula/foo: Permission denied
vagrant@wheezy:/vagrant$ 
vagrant@wheezy:/vagrant$ ## Root can write
vagrant@wheezy:/vagrant$ echo "abcd" | sudo tee $FOO
abcd
vagrant@wheezy:/vagrant$ cat $FOO
abcd
vagrant@wheezy:/vagrant$ echo "xyz" | sudo tee $FOO
xyz
vagrant@wheezy:/vagrant$ cat $FOO
xyz
vagrant@wheezy:/vagrant$ 
vagrant@wheezy:/vagrant$ ## Write up to one page size
vagrant@wheezy:/vagrant$ python -c 'import sys; sys.stdout.write("A" * 4095)' | sudo tee $FOO
@wheezy:/vagrant$ cat $FOO; echo

vagrant@wheezy:/vagrant$ ## Write over one page size
vagrant@wheezy:/vagrant$ ## sysfs only allows writes of PAGE_SIZE, the result here will be a 
vagrant@wheezy:/vagrant$ ## length of len(buf) % PAGE_SIZE
vagrant@wheezy:/vagrant$ python -c 'import sys; sys.stdout.write("B" * 5096)' | sudo tee $FOO
@wheezy:/vagrant$ [[ $(cat /sys/kernel/eudyptula/foo | wc -c) -eq 1000 ]] && echo "PASS"
vagrant@wheezy:/vagrant$ 
vagrant@wheezy:/vagrant$ # Clean up all eudyptula debugfs files
vagrant@wheezy:/vagrant$ sudo rmmod task09.ko
vagrant@wheezy:/vagrant$ ls -l /sys/kernel/eudyptula
ls: cannot access /sys/kernel/eudyptula: No such file or directory
